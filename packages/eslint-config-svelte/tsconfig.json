{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "compilerOptions": {
        "target": "es2020",
        "module": "es2022",
        "baseUrl": "./",
        "types": ["svelte"],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["fixtures/**/*.ts", "fixtures/**/*.svelte"],
    "exclude": ["node_modules/**"]
}
