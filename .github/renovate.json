{
    "extends": ["local>forsakringskassan/renovate-config"],
    "packageRules": [
        {
            "matchDepTypes": ["dependencies"],
            "rangeStrategy": "pin",
            "matchPackageNames": [
                "@eslint/**",
                "@rushstack/eslint-patch",
                "@typescript-eslint/**",
                "@vue/**",
                "eslint",
                "/^eslint-/"
            ]
        }
    ]
}
